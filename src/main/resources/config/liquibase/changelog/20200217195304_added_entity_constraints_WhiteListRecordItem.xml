<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity WhiteListRecordItem.
    -->
    <changeSet id="20200217195304-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="record_id"
                                 baseTableName="white_list_record_item"
                                 constraintName="fk_white_list_record_item_record_id"
