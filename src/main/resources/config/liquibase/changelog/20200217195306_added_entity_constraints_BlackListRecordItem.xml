<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity BlackListRecordItem.
    -->
    <changeSet id="20200217195306-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="record_id"
                                 baseTableName="black_list_record_item"
                                 constraintName="fk_black_list_record_item_record_id"
                                 referencedColumnNames="id"
                                 referencedTableName="record"/>

        <addForeignKeyConstraint baseColumnNames="person_id"
                                 baseTableName="black_list_record_item"
                                 constraintName="fk_black_list_record_item_person_id"
                                 referencedColumnNames="id"
                                 referencedTableName="black_list_person"/>

    </changeSet>
</databaseChangeLog>
